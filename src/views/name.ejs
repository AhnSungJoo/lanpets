<% include header %>
        <!-- Begin Page Content -->
<style>
    .dropdown {
      position: relative;
      display: inline-block;
    }
    
    .dropdown-content {
      display: none;
      position: absolute;
      background-color: #f9f9f9;
      min-width: 160px;
      box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
      padding: 12px 16px;
      z-index: 1;
    }
    
    .dropdown:hover .dropdown-content {
      display: block;
    }
</style>
<script>
    function replace() {
    let originName = $('originName option:selected').text();
    $.ajax({
      type: "POST",
      url: "/overview/name/replace",
      dataType: 'json',
      data: {data: originName},
      success: function(data) {
      }
    });
  }

</script>
  <% let nameList = locals.nameList %>
  <div class="container-fluid">

    <!-- Page Heading -->
    <h1></h1>
    <h1 class="h3 mb-2 text-gray-800"> Algorithm Name Overview</h1>
    <p class="mb-4">각 Algorithm ID의 대체 이름을 모두 보여줍니다.</p>

    <!-- DataTales Example -->
    <div class="card shadow mb-4">
      <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Algorithm Name List</h6>
      </div>
      <div class="card-body">
        <div class="table-responsive">
          <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
            <thead>
              <tr>
                <th>Algorithm ID</th>
                <th>Replace Name</th>
              </tr>
            </thead>
            <tbody>
                <% for (let index in nameList) { %>
                  <tr>
                      <td><%= nameList[index]['algorithm_id']  %></td>
                      <td><%= nameList[index]['algorithm_name'] %></td>
                  </tr>
                <% } %> 
            </tbody>
            </table>
          </div>
        </div>
      </div>
    <!-- table Over-->
    <div class="card shadow mb-4">
      <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">알고리즘 이름 변경하기</h6>
      </div>
      <div class="card-body">
        <div class="table-responsive">
          <form class="user" action="/overview/name/replace" method="POST">
            <p> 바꾸시려는 이름을 선택하세요 </p>
            <% for (let index in nameList) { %>
              <input type="radio" value=<%= nameList[index]['algorithm_name'] %> name="originName"><%= nameList[index]['algorithm_name'] %></input>
            <% } %> 
            <input type="text" class="form-control form-control-user" id="replaceName" name="replaceName" placeholder="Enter Repalce Name...">
            </div>
            <button type="submit" class="btn btn-primary">
              이름 변경하기
            </button>
          </form>
        </div>
      </div>
      </div>
    </div>
    <!-- 이름변경 over-->
  </div>
  </div>
  </div>

  
      <!-- End of Main Content -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>

  <!-- Bootstrap core JavaScript-->
  <script src="/resource/vendor/jquery/jquery.min.js"></script>
  <script src="/resource/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

  <!-- Core plugin JavaScript-->
  <script src="/resource/vendor/jquery-easing/jquery.easing.min.js"></script>

  <!-- Custom scripts for all pages-->
  <script src="/resource/js/sb-admin-2.min.js"></script>

  <!-- Page level plugins -->
  <script src="/vendor/chart.js/Chart.min.js"></script>

  <!-- Page level custom scripts -->
  <script src="/resource/js/demo/chart-area-demo.js"></script>
  <script src="/resource/js/demo/chart-pie-demo.js"></script>
  <% include footer %>
