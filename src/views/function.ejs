<% include newApp_header %>


            <!-- Form Start -->
            <div class="container-fluid pt-4 px-4">
                <div class="row g-4">
                    <div class="col-sm-12 col-xl-6">
                        <div class="bg-secondary rounded h-100 p-4">
                            <h6 class="mb-4">출금신청</h6>
                            <form class="user" action="/newApp/function/minusPoint" method="POST">
                                <div class="mb-3">
                                    <label class="form-label">유저 아이디</label>
                                    <input type="text" class="form-control"
                                        aria-describedby="emailHelp" id="kakaoId" name="kakaoId">
                                </div>
                                <div class="mb-3">
                                    <label class="form-label">차감포인트</label>
                                    <input type="text" class="form-control" " id="pointVal" name="pointVal">
                                    <input type="text" class="form-control form-control-user" id="pointFlag" name="pointFlag" value="both" hidden>
                                </div>
                                <button type="submit" class="btn btn-primary">포인트 차감하기</button>
                            </form>
                        </div>
                    </div>
                    <div class="col-sm-12 col-xl-6">
                        <div class="bg-secondary rounded h-100 p-4">
                            <h6 class="mb-4">포인트 강제차감</h6>
                            <form class="user" action="/newApp/function/minusPoint" method="POST">
                                <div class="mb-3">
                                    <label class="form-label">유저 아이디</label>
                                    <input type="text" class="form-control"
                                        aria-describedby="emailHelp" id="kakaoId" name="kakaoId">
                                </div>
                                <div class="mb-3">
                                    <label class="form-label">차감포인트</label>
                                    <input type="text" class="form-control" " id="pointVal" name="pointVal">
                                    <input type="text" class="form-control form-control-user" id="pointFlag" name="pointFlag" value="only" hidden>
                                </div>
                                <button type="submit" class="btn btn-primary">포인트 차감하기</button>
                            </form>
                        </div>
                    </div>
                    <div class="col-sm-12 col-xl-6">
                        <div class="bg-secondary rounded h-100 p-4">
                            <h6 class="mb-4">포인트 추가</h6>
                            <form class="user" action="/newApp/function/plusPoint" method="POST">
                                <div class="mb-3">
                                    <label class="form-label">유저 아이디</label>
                                    <input type="text" class="form-control"
                                        aria-describedby="emailHelp" id="kakaoId" name="kakaoId">
                                </div>
                                <div class="mb-3">
                                    <label class="form-label">추가포인트</label>
                                    <input type="text" class="form-control" " id="pointVal" name="pointVal">

                                </div>
                                <button type="submit" class="btn btn-primary">포인트 추가하기</button>
                            </form>
                        </div>
                    </div>
                    <div class="col-sm-12 col-xl-6">
                        <div class="bg-secondary rounded h-100 p-4">
                            <h6 class="mb-4">MMS 전송하기</h6>
                            <form class="user" action="/newApp/function/sendKakaoMsgComplainer" method="POST">
                                <div class="mb-3">
                                    <label for="form-label">유저 아이디</label>
                                    <input type="text" class="form-control" id="kakaoId" name="kakaoId">    
                                </div>
                                <div class="mb-3">
                                    <label for="form-label">메시지</label>
                                    <textarea class="form-control form-control-user" id="msg" name="msg"> </textarea>
                                </div>
                                <button type="submit" class="btn btn-primary">
                                   메시지 전송
                                </button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Form End -->


            <%include newApp_footer %>
        </div>
        <!-- Content End -->


        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="lib/chart/chart.min.js"></script>
    <script src="lib/easing/easing.min.js"></script>
    <script src="lib/waypoints/waypoints.min.js"></script>
    <script src="lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="lib/tempusdominus/js/moment.min.js"></script>
    <script src="lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

    <!-- Template Javascript -->
    <script src="js/main.js"></script>
</body>

</html>